{
  "name": "softkave-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build && npm run serve-distribution",
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "serve-distribution": "node dist/index.js",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon -r dotenv/config --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "test": "jest",
    "render-email-templates": "tsc --project email-templates/tsconfig.json && node email-templates/code/email-templates/index.js"
  },
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/express-graphql": "^0.8.0",
    "@types/express-jwt": "0.0.42",
    "@types/graphql": "^14.2.2",
    "@types/jest": "^24.0.15",
    "@types/joi": "^14.3.3",
    "@types/lodash": "^4.14.136",
    "@types/mongoose": "^5.5.8",
    "@types/node": "^12.6.2",
    "@types/randomcolor": "^0.5.2",
    "@types/uuid": "^3.4.5",
    "@types/validator": "^10.11.1",
    "argon2": "0.19.3",
    "async-validator": "1.10.1",
    "aws-sdk": "2.405.0",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "express-graphql": "0.6.12",
    "express-jwt": "5.3.1",
    "graphql": "0.13.2",
    "joi": "14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lit-html": "1.0.0",
    "lodash": "^4.17.14",
    "moment": "^2.24.0",
    "mongoose": "^5.7.8",
    "randomcolor": "0.5.4",
    "typescript": "^3.5.3",
    "uuid": "3.3.2",
    "validator": "10.11.0",
    "winston": "3.2.1",
    "winston-transport": "4.3.0"
  },
  "devDependencies": {
    "foreman": "^3.0.1",
    "jest": "^24.9.0",
    "nodemon": "^1.18.4",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0"
  },
  "keywords": []
}
